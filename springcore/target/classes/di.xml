<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean class="com.te.springcore.bean.Address" id="address">
		<property name="hno" value="231/A" />
		<property name="city" value="Bengaluru" />
		<property name="pincode" value="560085" />
		<property name="country" value="India" />
	</bean>
	
	<bean class="com.te.springcore.bean.Address" id="address1">
		<property name="hno" value="201/A" />
		<property name="city" value="Bengaluru" />
		<property name="pincode" value="560085" />
		<property name="country" value="India" />
	</bean>

	<bean class="com.te.springcore.bean.Employee" id="employee">
		<property name="empId" value="100" />
		<property name="empName" value="Nilim" />
		<!-- <property name="address" ref="address" /> -->
	</bean>
</beans>